<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- 
        Git repository
  -->
  <remote fetch="https://github.com/" name="git" review="review.cyanogenmod.com" />
  <!--
        Remove unwanted projects
  -->
  <remove-project name="platform/prebuilts/clang/darwin-x86/host/3.6" />
  <remove-project name="platform/prebuilts/gcc/darwin-x86/arm/arm-eabi-4.8" />
  <remove-project name="platform/prebuilts/gcc/darwin-x86/arm/arm-linux-androideabi-4.9" />
  <remove-project name="platform/prebuilts/gcc/darwin-x86/host/headers" />
  <remove-project name="platform/prebuilts/gcc/darwin-x86/host/i686-apple-darwin-4.2.1" />
  <remove-project name="platform/prebuilts/gcc/darwin-x86/mips/mips64el-linux-android-4.9" />
  <remove-project name="platform/prebuilts/gcc/darwin-x86/x86/x86_64-linux-android-4.9" />
  <remove-project name="platform/prebuilts/gcc/darwin-x86/aarch64/aarch64-linux-android-4.9" />

  <!--
        Remove those we want to replace
  -->
  <remove-project name="frameworks_av" />
  <remove-project name="hardware_qcom_camera" />
  <remove-project name="device_qcom_sepolicy" />

  <!--
        Add those we want to replace
  -->
  <!-- To support Snap Dragon Camera, we need media profiles that CM implements,
  AOSP does not implement those
  -->
  <project path="frameworks/av" name="CyanogenMod/android_frameworks_av" remote="git" groups="pdk" revision="cm-13.0" />
  <project path="hardware/qcom/camera" name="CyanogenMod/android_hardware_qcom_camera" remote="git" groups="qcom" revision="cm-13.0" />
  <project path="device/qcom/sepolicy" name="CyanogenMod/android_device_qcom_sepolicy" remote="git" revision="cm-13.0" />

  <!--
        Extras
  -->
  <project path="packages/apps/FMRadio" name="CyanogenMod/android_packages_apps_FMRadio" remote="git" revision="cm-13.0" />
  <project path="packages/apps/Snap" name="CyanogenMod/android_packages_apps_Snap" remote="git" revision="cm-13.0" />

  <!--
        WT88047 specific device tree, proprietary blobs and kernel
  -->
  <project name="Tesla-M-Devices/android_device_wingtech_wt88047" path="device/wingtech/wt88047" remote="git" revision="mm6.0" />
  <project name="premaca/proprietary_vendor_wingtech" path="vendor/wingtech" remote="git" revision="cm-13.0" />
  <project name="premaca/android_kernel_cyanogen_msm8916" path="kernel/cyanogen/msm8916" remote="git" revision="cm-13.0-amss-2.1-wt88047" />
  <project name="premaca/android_device_cyanogen_msm8916-common" path="device/cyanogen/msm8916-common" remote="git" revision="cm-13.0"/>

  <!--
        Dependencies
  -->
  <project name="CyanogenMod/android_external_stlport" path="external/stlport" remote="git" revision="cm-13.0" />
  <project name="CyanogenMod/android_vendor_qcom_opensource_dataservices" path="vendor/qcom/opensource/dataservices"  remote="git" revision="cm-13.0" />

</manifest>
